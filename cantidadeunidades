#include <iostream>

using namespace std;

int main() {
    int un, pr, subtt;

    cout << "Ingresa la cantidad de unidades: ";
    cin >> un;
    cout << "Cantidad de unidades: " << un << endl;
    
    cout << "Ingresa el precio: ";
    cin >> pr;
    cout << "Precio: " << pr << endl;
    
    subtt = un * pr;
    cout << "Subtotal: " << subtt << endl;
    
    return 0;
}